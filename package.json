{
  "name": "magic-ui",
  "version": "1.0.0",
  "description": "基于WebGL的拖拽式生成UI界面",
  "main": "dist/main/main.prod.js",
  "scripts": {
    "start": "npm run build && electron ./dist/main/main.prod.js",
    "start-main": "electron ./dist/main/main.prod.js",
    "start-dev": "npm run dev-renderer",
    "start-main-dev": "npm run dev-main && electron ./dist/main/main.dev.js",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "dev-renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack.renderer.dev.config.js",
    "dev-main": "cross-env NODE_ENV=development webpack --config webpack.main.dev.config.js",
    "package": "npm run build && electron-builder build --publish never",
    "package-win": "npm run build && electron-builder build --win --x64",
    "package-linux": "npm run build && electron-builder build --linux",
    "package-mac": "npm run build && electron-builder build --mac",
    "electron-rebuild": "electron-rebuild",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Magic UI",
    "appId": "magic.ui",
    "files": [
      "dist/",
      "node_modules/",
      "resources/",
      "native/",
      "index.html",
      "main.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "./resources/icons/app.icns"
    },
    "dmg": {
      "backgroundColor": "#ffffff",
      "icon": "./resources/icons/app.icns",
      "iconSize": 80,
      "title": "magic ui"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "icon": "./resources/icons/app.png",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "./resources/icons",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundial-dreams/autoui.git"
  },
  "author": "sundial-dreams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sundial-dreams/autoui/issues"
  },
  "homepage": "https://github.com/sundial-dreams/autoui#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/plugin-transform-react-inline-elements": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/electron": "^1.6.10",
    "@types/history": "^4.7.5",
    "@types/marked": "^0.7.4",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/request": "^2.48.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.41.10",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^3.4.2",
    "devtron": "^1.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-rebuild": "^1.10.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-addon-api": "^2.0.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@hot-loader/react-dom": "^16.13.0",
    "codemirror": "^5.52.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "history": "^4.10.1",
    "konva": "^4.2.2",
    "marked": "^1.1.0",
    "postinstall": "^0.7.0",
    "psd": "^3.2.0",
    "react": "^16.13.1",
    "react-codemirror2": "^7.1.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "socket.io-client": "^2.3.0",
    "three": "^0.115.0",
    "three-dragcontrols": "^0.88.2"
  }
}
